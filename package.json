{
  "name": "tetu-game-story",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "typeorm:generate": "npx typeorm-ts-node-esm migration:generate -d src/migrations/db.source.ts src/migrations/data/StoryMigrate",
    "typeorm:migrate": "npx typeorm-ts-node-esm migration:run -d src/migrations/db.source.ts",
    "typeorm:revert": "npx typeorm-ts-node-esm migration:revert -d src/migrations/db.source.ts",
    "typeorm:drop": "npx typeorm-ts-node-esm schema:drop -d src/migrations/db.source.ts",
    "start": "node build/app.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/app.ts",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.{ts}'"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "prettier": "^3.1.1",
    "reflect-metadata": "^0.2.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.19",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.7",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2"
  }
}
